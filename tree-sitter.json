{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "curl",
      "camelcase": "Curl",
      "title": "Curl",
      "scope": "source.curl",
      "file-types": [
        "curl"
      ],
      "injection-regex": "^curl$",
      "class-name": "TreeSitterCurl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Curl grammar for tree-sitter",
    "authors": [
      {
        "name": "jialtang"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-curl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}